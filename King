package me.hacksmuch.pieces;

import javafx.scene.image.Image;
import me.hacksmuch.Chess;
import me.hacksmuch.Location;

public class King extends Piece
{
	public King(Chess chess, Image image, Team team, Location startlocation)
	{
		super(chess, image, team, startlocation);
	}

	void markPossibleMoves()
	{
		getMoves().applyKingMoves(this, getLocation());
	}
}
